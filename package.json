{
  "name": "gestao-acesso-api",
  "version": "0.0.64",
  "description": "API Gest√£o de Acesso",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "fixture-NOT-WORK": "ts-node --project ./tsconfig.build.json ./src/fixture/index.ts",
    "fixture": "ts-node -r tsconfig-paths/register ./src/fixture/index.ts",
    "fixture:dev": "ts-node-dev -r tsconfig-paths/register ./src/fixture.ts",
    "deploy": "npm run deploy:recreate -- gestao-acesso-api",
    "deploy:recreate": "docker compose -f '../deploy/docker-compose-prod.yml' up -d --build --force-recreate",
    "docker:debug": "docker compose -f './.docker/qg-piloto-dev/docker-compose.debug.yml' up -d --build --force-recreate",
    "sudo:docker:debug": "sudo docker compose -f './.docker/qg-piloto-dev/docker-compose.debug.yml' up -d --build --force-recreate",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:clear": "jest --clearCache",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:watch": "jest --watch --config ./test/jest-e2e.json",
    "git:clu": "git config --local user.name \"Haroldo Cruz\" ; git config --local user.email \"haroldocruzdr@gmail.com\"",
    "git:upd": "git pull --recurse-submodules",
    "git:upd:sub": "git submodule update --remote",
    "github:key": "git remote add origin git@hc-github:/sigilodigital/qg-gestao-acesso-api.git",
    "github:pass": "git remote add origin https://github.com/sigilodigital/qg-gestao-acesso-api.git",
    "find:pack:update": "npx ncu",
    "circular:madge": "npx madge --circular --extensions ts ./",
    "circular:dpdm": "npx dpdm ./src/main.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.1.3",
    "@nestjs/swagger": "^7.1.4",
    "@nestjs/typeorm": "^10.0.0",
    "@types/request": "^2.48.8",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cpf-cnpj-validator": "^1.0.3",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.3.1",
    "jwt-decode": "^3.1.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "restpki-client": "^2.3.3",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "typeorm-model-generator": "^0.4.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/plugin-syntax-decorators": "^7.23.3",
    "@nestjs/cli": "^10.1.11",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.1.3",
    "@playwright/test": "^1.44.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.3",
    "@types/joi": "^17.2.3",
    "@types/node": "^20.4.5",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "dpdm": "^3.14.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "29.6.2",
    "madge": "^6.1.0",
    "npm-check-updates": "^16.14.20",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/",
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1",
      "^@sd-root(|/.*)$": "<rootDir>/$1",
      "^@libs/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@libs/audit(|/.*)$": "<rootDir>/libs/audit/src/$1"
    }
  }
}
